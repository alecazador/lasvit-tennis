{
  "root": true,
  "api": {
    "projectId": "3g1sulyv",
    "dataset": "production"
  },
  "project": {
    "name": "Lasvit Tennis CMS",
    "basePath": "/studio"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/color-input",
    "media-library",
    "color-list",
    "intl-input",
    "autocomplete-tags"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/desk-tool/structure",
      "path": "./src/deskStructure"
    },
    {
      "implements": "part:@sanity/production-preview/resolve-production-url",
      "path": "../utils/resolveProductionUrl.js"
    }
  ]
}
